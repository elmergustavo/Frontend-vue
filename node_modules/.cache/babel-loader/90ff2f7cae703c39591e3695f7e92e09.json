{"ast":null,"code":"//import Modal from '~/components/modal.vue'\nimport defaultpasos from '@/layouts/defaultpasos';\nexport default {\n  components: {\n    PokemonOptions,\n    PokemonPicture\n  },\n\n  data() {\n    return {\n      pokemonArr: [],\n      pokemon: null,\n      showPokemon: false,\n      showAnswer: false,\n      message: ''\n    };\n  },\n\n  methods: {\n    async mixPokemonArray() {\n      this.pokemonArr = await getPokemonOptions();\n      const rndInt = Math.floor(Math.random() * 4);\n      this.pokemon = this.pokemonArr[rndInt];\n    },\n\n    checkAnswer(selectedId) {\n      this.showPokemon = true;\n      this.showAnswer = true;\n\n      if (selectedId === this.pokemon.id) {\n        this.message = `Correcto, ${this.pokemon.name}`;\n      } else {\n        this.message = `Oops, era ${this.pokemon.name}`;\n      }\n    },\n\n    newGame() {\n      this.showPokemon = false;\n      this.showAnswer = false;\n      this.pokemonArr = [];\n      this.pokemon = null;\n      this.mixPokemonArray();\n    }\n\n  },\n\n  mounted() {\n    this.mixPokemonArray();\n  }\n\n};","map":{"version":3,"mappings":"AAoCA;AACA,OAAOA,YAAP,MAAyB,wBAAzB;AAGA,eAAe;AACXC,YAAU,EAAE;AAAEC,kBAAF;AAAkBC;AAAlB,GADD;;AAEXC,MAAI,GAAG;AACH,WAAO;AACHC,gBAAU,EAAE,EADT;AAEHC,aAAO,EAAE,IAFN;AAGHC,iBAAW,EAAE,KAHV;AAIHC,gBAAU,EAAE,KAJT;AAKHC,aAAO,EAAE;AALN,KAAP;AAOH,GAVU;;AAWXC,SAAO,EAAE;AACL,UAAMC,eAAN,GAAwB;AACpB,WAAKN,UAAL,GAAkB,MAAMO,iBAAiB,EAAzC;AAEA,YAAMC,MAAK,GAAIC,IAAI,CAACC,KAAL,CAAYD,IAAI,CAACE,MAAL,KAAgB,CAA5B,CAAf;AACA,WAAKV,OAAL,GAAe,KAAKD,UAAL,CAAiBQ,MAAjB,CAAf;AACH,KANI;;AAOLI,eAAW,CAAEC,UAAF,EAAe;AACtB,WAAKX,WAAL,GAAmB,IAAnB;AACA,WAAKC,UAAL,GAAmB,IAAnB;;AAEA,UAAIU,UAAS,KAAM,KAAKZ,OAAL,CAAaa,EAAhC,EAAqC;AACjC,aAAKV,OAAL,GAAgB,aAAa,KAAKH,OAAL,CAAac,IAAM,EAAhD;AACJ,OAFA,MAEO;AACH,aAAKX,OAAL,GAAgB,aAAa,KAAKH,OAAL,CAAac,IAAM,EAAhD;AACJ;AACH,KAhBI;;AAiBLC,WAAO,GAAG;AAEN,WAAKd,WAAL,GAAmB,KAAnB;AACA,WAAKC,UAAL,GAAmB,KAAnB;AACA,WAAKH,UAAL,GAAmB,EAAnB;AACA,WAAKC,OAAL,GAAmB,IAAnB;AACA,WAAKK,eAAL;AAEJ;;AAzBK,GAXE;;AAsCXW,SAAO,GAAG;AACN,SAAKX,eAAL;AACJ;;AAxCW,CAAf","names":["defaultpasos","components","PokemonOptions","PokemonPicture","data","pokemonArr","pokemon","showPokemon","showAnswer","message","methods","mixPokemonArray","getPokemonOptions","rndInt","Math","floor","random","checkAnswer","selectedId","id","name","newGame","mounted"],"sourceRoot":"","sources":["/home/gustavo/Escritorio/fronted-tutores/src/views/PasosView.vue"],"sourcesContent":["<template>\n<defaultpasos />\n    <div> \n    <div class=\"container\">\n      <div class=\"buttom-form\">\n            <div class=\"row\">\n              <card\n                imagen=\"/image/nombre.png\"\n                message=\"Ingresa tu nombre completo para saber cómo dirigirnos hacia ti.\"\n              />\n              <card\n                imagen=\"/image/descripcion.png\"\n                message=\"Descríbete en palabras para que tus futuros alumnos te conozcan brevemente.\"\n              />\n              <card\n                imagen=\"/image/gradoacademico.png\"\n                message=\"Anota el grado académico que tienes para conocer tus mejores habilidades.\"\n              />\n              <card\n                imagen=\"/image/habilidades.png\"\n                message=\"Anota con precisión tus habilidades profesionales para ayudarte a conseguir más trabajo.\"\n              />\n              <card \n                imagen=\"/image/datoscorreocelu.png\" \n                message=\" Agrega credibilidad proporcionando tus datos de teléfono o celular y correo electrónico para mantener una comunidad segura.\" \n              />\n            </div>\n      </div>\n    \n      <a href=\"cursos\" class=\"btn btn-outline-success\" style=\"margin-left: 100%; margin: 0.1cm\">Siguiente</a>\n    </div>\n    \n  </div>\n</template>\n\n<script>\n//import Modal from '~/components/modal.vue'\nimport defaultpasos from '@/layouts/defaultpasos'\n\n\nexport default {\n    components: { PokemonOptions, PokemonPicture },\n    data() {\n        return {\n            pokemonArr: [],\n            pokemon: null,\n            showPokemon: false,\n            showAnswer: false,\n            message: ''\n        }\n    },\n    methods: {\n        async mixPokemonArray() {\n            this.pokemonArr = await getPokemonOptions()\n\n            const rndInt = Math.floor( Math.random() * 4 )\n            this.pokemon = this.pokemonArr[ rndInt ]\n        },\n        checkAnswer( selectedId ) {\n            this.showPokemon = true\n            this.showAnswer  = true\n\n            if( selectedId === this.pokemon.id ) {\n                this.message = `Correcto, ${ this.pokemon.name }`\n            } else {\n                this.message = `Oops, era ${ this.pokemon.name }`\n            }\n        },\n        newGame() {\n\n            this.showPokemon = false\n            this.showAnswer  = false\n            this.pokemonArr  = []\n            this.pokemon     = null\n            this.mixPokemonArray()            \n\n        }\n    },\n    mounted() {\n        this.mixPokemonArray()\n    }\n\n}\n</script>\n\n<style scoped>\n.card {\n  margin: 1em;\n  padding: 0.3cm;\n  float: left;\n}\n\n.card-text {\n  text-align: center;\n  font-size: 0.1vw;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}