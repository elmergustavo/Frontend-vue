{"ast":null,"code":"export default {\n  data() {\n    return {\n      habilidades: '',\n      habilidadesState: null,\n      fields: ['nombre', 'apellido', 'habilidades'],\n      submittedCursos: []\n    };\n  },\n\n  methods: {\n    checkFormValidity() {\n      const valid = this.$refs.form.checkValidity();\n      this.nombreState = valid;\n      this.apellidoState = valid;\n      this.descriptionState = valid;\n      this.GradoState = valid;\n      this.habilidadesState = valid;\n      this.correoState = valid;\n      this.celularState = valid;\n      return valid;\n    },\n\n    resetModal() {\n      this.nombre = '';\n      this.nombreState = null;\n      this.apellido = '';\n      this.apellidoState = null;\n      this.description = '';\n      this.descriptionState = null;\n      this.Grado = '';\n      this.GradoState = null;\n      this.habilidades = '';\n      this.habilidadesState = null;\n      this.celular = '';\n      this.celularState = null;\n      this.correo = '';\n      this.correoState = null;\n    },\n\n    handleOk(bvModalEvt) {\n      // Prevent modal from closing\n      bvModalEvt.preventDefault(); // Trigger submit handler\n\n      this.handleSubmit();\n    },\n\n    handleSubmit() {\n      // Exit when the form isn't valid\n      if (!this.checkFormValidity()) {\n        return;\n      } // Push the name to submitted names\n\n\n      this.submittedCursos.push({\n        nombre: this.nombre,\n        apellido: this.apellido,\n        habilidades: this.habilidades\n      });\n      console.log(this.submittedCursos); // Hide the modal manually\n\n      this.$nextTick(() => {\n        this.$bvModal.hide('modal-prevent-closing');\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA0HE,eAAe;AACbA,MAAI,GAAG;AACL,WAAO;AACLC,iBAAW,EAAE,EADR;AAELC,sBAAgB,EAAE,IAFb;AAGLC,YAAM,EAAC,CAAC,QAAD,EAAW,UAAX,EAAuB,aAAvB,CAHF;AAILC,qBAAe,EAAE;AAJZ,KAAP;AAMD,GARY;;AASbC,SAAO,EAAE;AACPC,qBAAiB,GAAG;AAClB,YAAMC,KAAI,GAAI,KAAKC,KAAL,CAAWC,IAAX,CAAgBC,aAAh