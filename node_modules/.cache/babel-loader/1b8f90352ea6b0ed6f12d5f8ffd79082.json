Modal\"\n      @hidden=\"resetModal\"\n      @ok=\"handleOk\"\n    >\n      <form ref=\"form\" @submit.stop.prevent=\"handleSubmit\">\n        <b-form-group\n          label=\"Ingresa tus Nombres:\"\n          label-for=\"nombre-input\"\n          invalid-feedback=\"Llenar este campo\"\n          :state=\"nombreState\"\n        >\n          <b-form-input\n            type=\"text\"\n            id=\"nombre-input\"\n            v-model=\"nombre\"\n            :state=\"nombreState\"\n            required\n          ></b-form-input>\n        </b-form-group>\n\n        <b-form-group\n          label=\"Ingresa tus Apellidos:\"\n          label-for=\"apellido-input\"\n          invalid-feedback=\"Llenar este campo\"\n          :state=\"apellidoState\"\n        >\n          <b-form-input\n            type=\"text\"\n            id=\"apellido-input\"\n            v-model=\"apellido\"\n            :state=\"apellidoState\"\n            required\n          ></b-form-input>\n        </b-form-group>\n\n        <b-form-group\n          label=\"Ingresa una Descripción:\"\n          label-for=\"description-input\"\n          invalid-feedback=\"Llenar este campo\"\n          :state=\"descriptionState\"\n        >\n          <textarea \n            class=\"form-control\" \n            id=\"description-text\" \n            required\n            :state=\"descriptionState\"></textarea>\n        </b-form-group>\n\n        <b-form-group\n          label=\"Ingresa tu Grado Académico:\"\n          label-for=\"Grado-input\"\n          invalid-feedback=\"Llenar este campo\"\n          :state=\"GradoState\"\n        >\n          <b-form-input\n            type=\"text\"\n            id=\"description-input\"\n            v-model=\"Grado\"\n            required\n            :state=\"GradoState\"\n          ></b-form-input>\n        </b-form-group>\n\n        <b-form-group\n          label=\"Describe tus habilidades:\"\n          label-for=\"habilidades-input\"\n          invalid-feedback=\"Llenar este campo\"\n          :state=\"habilidadesState\"\n        >\n          <b-form-input\n            id=\"habilidades-input\"\n            v-model=\"habilidades\"\n            required\n            :state=\"habilidadesState\"\n          ></b-form-input>\n        </b-form-group>\n        \n        <b-form-group\n          label=\"Ingresa tu celular:\"\n          label-for=\"celular-input\"\n          invalid-feedback=\"Llenar este campo\"\n          :state=\"celularState\"\n        >\n          <b-form-input\n            type=\"tel\"\n            id=\"celular-input\"\n            v-model=\"celular\"\n            required\n            :state=\"celularState\"\n          ></b-form-input>\n        </b-form-group>\n\n        <b-form-group\n          label=\"Ingresa tu correo:\"\n          label-for=\"correo-input\"\n          invalid-feedback=\"Llenar este campo\"\n          :state=\"correoState\"\n        >\n          <b-form-input\n            type=\"email\"\n            id=\"correo-input\"\n            v-model=\"correo\"\n            required\n            :state=\"correoState\"\n          ></b-form-input>\n        </b-form-group>\n      </form>\n    </b-modal>\n  </div>\n</template>\n\n<script>\n  export default {\n    data() {\n      return {\n        habilidades: '',\n        habilidadesState: null,\n        fields:['nombre', 'apellido' ,'habilidades' ],\n        submittedCursos: []\n      }\n    },\n    methods: {\n      checkFormValidity() {\n        const valid = this.$refs.form.checkValidity()\n        this.nombreState = valid\n        this.apellidoState = valid\n        this.descriptionState = valid\n        this.GradoState = valid\n        this.habilidadesState = valid\n        this.correoState = valid\n        this.celularState = valid\n        return valid\n      },\n      resetModal() {\n        this.nombre = ''\n        this.nombreState = null\n        this.apellido = ''\n        this.apellidoState = null\n        this.description = ''\n        this.descriptionState = null\n        this.Grado = ''\n        this.GradoState = null\n        this.habilidades = ''\n        this.habilidadesState = null\n        this.celular = ''\n        this.celularState = null\n        this.correo = ''\n        this.correoState = null\n      },\n      handleOk(bvModalEvt) {\n        // Prevent modal from closing\n        bvModalEvt.preventDefault()\n        // Trigger submit handler\n        this.handleSubmit()\n      },\n      handleSubmit() {\n        // Exit when the form isn't valid\n        if (!this.checkFormValidity()) {\n          return\n        }\n        // Push the name to submitted names\n        this.submittedCursos.push({ nombre: this.nombre, apellido: this.apellido, habilidades: this.habilidades })\n        console.log(this.submittedCursos)\n        // Hide the modal manually\n        this.$nextTick(() => {\n          this.$bvModal.hide('modal-prevent-closing')\n        })\n      }\n    }\n  }\n</script>\n\n<style scoped>\n#hr{\n  border-color: white;\n}\n</style>"]},"metadata":{},"sourceType":"module"}